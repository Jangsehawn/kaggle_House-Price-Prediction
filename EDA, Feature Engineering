    "nearest_neighbor = neighbor_info_df[(neighbor_info_df['data'] == 'train') & (neighbor_info_df['distance'] <= 4)].copy()\n",
    "nearest_neighbor = nearest_neighbor.sort_values(['id','grade_diff','sqft_living_diff','sqft_living15_diff',\n",
